	{

	}
	else if (ferz_placment_alg(array, x, y, n) == 0)
	{

	}
}

int main()
{
	int n;
	cin >> n;

	vector<vector<int>> board = createArray(n);

	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			clearArray(board);
			ferz_placment_alg(board, i, j, n);
		}
	}

	cout << "y";
	_getch();
}
